[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust-lib/debug/libgame.so"
linux.release.x86_64 =   "res://rust-lib/release/libgame.so"
windows.debug.x86_64 =   "res://rust-lib/debug/game.dll"
windows.release.x86_64 = "res://rust-lib/release/game.dll"
macos.debug =            "res://rust-lib/x86_64-apple-darwin/debug/libgame.dylib"
macos.release =          "res://rust-lib/x86_64-apple-darwin/release/libgame.dylib"
macos.debug.arm64 =      "res://rust-lib/aarch64-apple-darwin/debug/libgame.dylib"
macos.release.arm64 =    "res://rust-lib/aarch64-apple-darwin/release/libgame.dylib"
ios.debug =              "res://rust-lib/aarch64-apple-ios/debug/libgame.dylib"
ios.release =            "res://rust-lib/aarch64-apple-ios/release/libgame.dylib"